<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--BezelImageView-->
    <declare-styleable name="BezelImageView">
        <attr name="maskDrawable" format="reference"/>
        <attr name="borderDrawable" format="reference"/>
        <attr name="desaturateOnPress" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="FloatLabelLayout">
        <attr name="floatLabelTextAppearance" format="reference" />
        <attr name="floatLabelPaddingLeft" format="reference|dimension" />
        <attr name="floatLabelPaddingRight" format="reference|dimension" />
        <attr name="floatLabelPaddingTop" format="reference|dimension" />
        <attr name="floatLabelPaddingBottom" format="reference|dimension" />
        <!-- The hint to display in the floating label -->
        <attr name="floatLabelHint" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="FabView">

        <attr name="fab_attachTo" format="reference" />
        <attr name="fab_attachAt" format="enum">
            <enum name="top_left" value="1" />
            <enum name="top_right" value="2" />
            <enum name="bottom_left" value="3" />
            <enum name="bottom_right" value="4" />
        </attr>
        <attr name="fab_attachType" format="enum">
            <enum name="border" value="1" />
            <enum name="inside" value="2" />
        </attr>
        <attr name="fab_size" format="enum">
            <enum name="normal" value="1" />
            <enum name="small" value="2" />
        </attr>
        <attr name="fab_padding" format="dimension" />
        <attr name="fab_revealAfterMs" format="integer" />

    </declare-styleable>
</resources>